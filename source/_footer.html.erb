<div id="footer">
  <div class="container">
    <div class="row">
      <div class="col-xs-4">
        <ul class="list-inline">
          <% ['youtube', 'instagram', 'facebook', 'twitter'].each do |name|  %>
            <li>
              <% link_to "#" do %>
                <i class="fa fa-<%= name %>"></i>
              <% end %>
            </li>
          <% end %>
        </ul>
      </div>
      <div class="col-xs-8 text-right">
        <p>This is a Airbnb Home challenge @ LeWagon</p>
      </div>
    </div>
  </div>
</div>



